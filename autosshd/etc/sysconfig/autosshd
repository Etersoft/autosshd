HOST=127.0.0.1
LOCAL_PORT=22
REMOTE_PORT=2200
MON_INTERVAL=20000

OPTIONS=" autossh -2 -fN -M ${MON_INTERVAL} -R ${REMOTE_PORT}:localhost:${LOCAL_PORT} ${USER}@${HOST}"
